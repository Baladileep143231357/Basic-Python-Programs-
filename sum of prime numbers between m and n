#sum of prime numbers between m and n
m=int(input('enter m value: '))
n=int(input('enter n value: '))
sum=0;
for num in range(m,n+1):
    if num>1:
        is_prime=True
        for i in range(2,int(num**0.5)+1):
            if num%i==0:
                is_prime=False
                break
        if is_prime:
            sum+=num
print('sum of prime numbers between',m,'and',n,'is',sum)
